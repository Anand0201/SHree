                            //for getting no. of correct answers,wrong answers and for coloring the button (red,green) by ids.
                            function checkAnswer() {
                              choices = document.getElementsByName("choices");
                              for (var i = 0; i < choices.length; i++) {
                                if (choices[i].checked) {
                                  choice = choices[i].value;
                                  tick[pos] = choices[i].value;
                                  radcnt[pos] = tick[pos];
                                  cnt[pos]++;
                                  colour = 0;
                                  break;
                                } else {
                                  //document.getElementById('red').checked =false;
                                  colour = 1;
                                }
                              }

                              if (colour == 0) {
                                see = 1;
                                document.getElementById(
                                  palette[pos]
                                ).style.backgroundColor = "green";
                                document.getElementById(
                                  palette[pos]
                                ).style.borderRadius = "2px 8px 8px 2px";
                                document.getElementById("paper").innerHTML =
                                  "General Ability Test (GAT))";
                                if (
                                  radcnt[pos] == tick[pos] &&
                                  cnt[pos] == 1
                                ) {
                                  if (choice == questions[pos][4]) {
                                    correct++;
                                  } else {
                                    wrong++;
                                  }
                                }
                                //test case for counting wrong answer when clicked on save and next button
                                if (
                                  radcnt[pos] != tick[pos] &&
                                  cnt[pos] == 1
                                ) {
                                  if (choice == questions[pos][4]) {
                                    wrong--;
                                    correct++;
                                  } else {
                                    correct--;
                                    wrong++;
                                  }
                                }
                                //test case for counting when clicked on mark for review latter on save and next
                                if (
                                  purpletogreen[pos] == 1 &&
                                  radcnt[pos] == tick[pos]
                                ) {
                                  if (choice == questions[pos][4]) {
                                    book--;
                                  } else {
                                    book--;
                                  }
                                }
                                if (
                                  redtogreen[pos] == 1 &&
                                  radcnt[pos] == tick[pos]
                                ) {
                                  if (choice == questions[pos][4]) {
                                    nbook--;
                                  } else {
                                    nbook--;
                                  }
                                }

                                if (pos >= 20) {
                                  document.getElementById("paper").innerHTML =
                                    "General Ability Test (GAT";
                                }
                                // if (pos >= 39) {
                                //   document.getElementById("paper").innerHTML =
                                //     "GK";
                                // }
                                // if (pos >= 59) {
                                //   document.getElementById("paper").innerHTML =
                                //     "GK";
                                // }
                                pos++;
                                renderQuestion();
                              } else {
                                document.getElementById(
                                  palette[pos]
                                ).style.backgroundColor = "#EBEBEB";
                                pos++;
                                renderQuestion();
                              }
